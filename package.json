{
  "name": "cdk-template",
  "version": "0.1.0",
  "bin": {
    "cdk-template": "bin/cdk-template.js"
  },
  "scripts": {
    "format": "biome format --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "biome lint --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "check": "biome check --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "bootstrap": "cdk bootstrap",
    "deploy": "cdk deploy",
    "diff": "cdk diff",
    "destroy": "cdk destroy",
    "synth": "cdk synth"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.235.0",
    "constructs": "^10.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.19.2",
    "aws-cdk": "^2.1101.0",
    "jest": "^29.7.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.2.5",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2"
  }
}