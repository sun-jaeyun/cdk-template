{
  "name": "cdk-template",
  "version": "0.1.0",
  "bin": {
    "cdk-template": "bin/cdk-template.js"
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\" --fix --concurrency=auto",
    "typecheck": "tsc --noEmit",
